<head>
    <%
        var title = page.title;
        if (is_archive()){
            title = __('archive');
            if (is_month()){
                title += ': ' + page.year + '/' + page.month;
            } else if (is_year()){
                title += ': ' + page.year;
            }
        } else if (is_category()){
            title = __('category') + ': ' + page.category;
        } else if (is_tag()){
            title = __('tag') + ': ' + page.tag;
        }
        if (title) {
            title = title + " | " + config.title;
        } else {
            title = config.title;
        }
    %>
    <title><%= title %></title>
    <!--<base href="<%= config.url %>" />-->
    <%- partial('_partial/metas') %>
    <%- partial('_partial/css') %>
    <%- partial('_partial/js') %>
    <link rel="canonical" href="<%- url_for('/') %>" />
    <%- favicon_tag(theme.favicon) %>
</head>